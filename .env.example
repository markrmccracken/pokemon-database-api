# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=pokemondb
DB_USER=pokemonapi
DB_PASSWORD=YOUR_PASSWORD_HERE

# Server Configuration
NODE_ENV=production
PORT=3000
API_VERSION=v1

# Security
JWT_SECRET=your_jwt_secret_here
API_KEY=your_api_key_here

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# CORS
ALLOWED_ORIGINS=*

# Monitoring
LOG_LEVEL=info
